{
    "data": {
        "train_path": "../input/digit/train.csv",
        "test_path": "../input/digit/test.csv",
        "output_dir": ".",
        "type": "table",
        "pred_cols": ["label"],
        "id_col": "ImageId"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "digit",
                "methods": [
                    "add_image_id"
                ]
            },
            "missing_imputation": true,
            "category_encoding": {
                "default": {
                    "model": "onehot"
                },
                "options": [
                ]
            },
            "x_scaler": "standard"
        }
    },
    "fit": {
        "train_mode": "clf",
        "scoring": "accuracy",
        "modelname": "digit",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "svc",
                "n_trials": 5,
                "fit_params": {
                },
                "params": {
                    "probability": [true],
                    "C": [1, 2, 3, 4, 5],
                    "degree": [1, 2, 3, 4, 5],
                    "kernel": ["rbf", "linear", "poly", "sigmoid"]
                }
            }
        ]
    },
    "post": {
        "myfunc": "digit",
        "methods": [
            "translate_label2upper"
        ]
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
