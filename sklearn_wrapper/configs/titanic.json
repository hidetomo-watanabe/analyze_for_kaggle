{
    "data": {
        "train_path": "../input/titanic/train.csv",
        "test_path": "../input/titanic/test.csv",
        "output_dir": ".",
        "type": "table",
        "pred_cols": ["Survived"],
        "id_col": "PassengerId"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "titanic",
                "methods": [
                    "translate_honorific_title",
                    "translate_age",
                    "translate_familystatus"
                ]
            },
            "category_encoding": {
                "default": {
                    "model": "onehot"
                },
                "options": [
                    {
                        "columns": [
                            "Pclass",
                            "FamilyStatus"
                        ],
                        "model": "onehot"
                    }
                ]
            }
        }
    },
    "fit": {
        "train_mode": "clf",
        "scoring": "accuracy",
        "modelname": "titanic",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "log_reg",
                "missing_imputation": "mean",
                "dimension_reduction": {
                    "n": 500,
                    "model": "pca"
                },
                "x_scaler": "standard",
                "fit_params": {
                },
                "params": {
                    "random_state": [42]
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
