{
    "data": {
        "train_path": "./tests/house/train.csv",
        "test_path": "./tests/house/test.csv",
        "output_dir": "./outputs",
        "type": "table",
        "pred_cols": ["SalePrice"],
        "id_col": "Id"
    },
    "pre": {
        "train_mode": "reg",
        "y_pre": "log",
        "table": {
            "missing_imputation": true,
            "category_encoding": {
                "default": {
                    "model": "onehot"
                },
                "options": [
                ]
            },
            "dimension_reduction": {
                "n": 100,
                "model": "pca"
            }
        }
    },
    "fit": {
        "myfunc": "house",
        "train_mode": "reg",
        "scoring": "neg_mean_squared_error",
        "modelname": "tmp_house2",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "linear_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "keras_reg",
                "x_scaler": "standard",
                "cv_select": "train_all",
                "fit_params": {
                    "epochs": 3,
                    "batch_size": 50
                },
                "params": {
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
