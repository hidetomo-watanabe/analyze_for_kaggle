{
    "data": {
        "train_path": "./tests/titanic/train.csv",
        "test_path": "./tests/titanic/test.csv",
        "output_dir": "./outputs",
        "type": "table",
        "pred_cols": ["Survived"],
        "id_col": "PassengerId"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "titanic",
                "methods": [
                    "translate_title",
                    "translate_age",
                    "translate_familysize"
                ]
            },
            "deletion": [
                "Name",
                "Ticket",
                "Fare",
                "Cabin",
                "Embarked"
            ],
            "category_encoding": {
                "default": {
                    "model": "label"
                },
                "options": [
                    {
                        "columns": [
                            "Pclass",
                            "FamilySize"
                        ],
                        "model": "label"
                    }
                ]
            }
        }
    },
    "fit": {
        "train_mode": "clf",
        "scoring": "accuracy",
        "cv": {
            "fold": "stratifiedk",
            "num": 3
        },
        "modelname": "tmp_titanic2",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "lgb_clf",
                "missing_imputation": "mean",
                "fit_params": {
                    "eval_metric": "accuracy",
                    "early_stopping_rounds": 10
                },
                "params": {
                    "n_estimators": [5],
                    "max_depth": [3]
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
