{
    "data": {
        "train_path": "./tests/titanic/train.csv",
        "test_path": "./tests/titanic/test.csv",
        "output_dir": "./outputs",
        "type": "table",
        "pred_cols": ["Survived"],
        "id_col": "PassengerId"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "titanic",
                "methods": [
                    "translate_title",
                    "translate_age",
                    "translate_familysize"
                ]
            },
            "deletion": [
                "Title",
                "Name",
                "Ticket",
                "Fare",
                "Cabin",
                "Embarked"
            ],
            "category_encoding": {
                "default": {
                    "model": "onehot"
                },
                "options": [
                    {
                        "columns": [
                            "FamilySize"
                        ],
                        "model": "onehot"
                    }
                ]
            },
            "adversarial_validation": {
                "model_config": {
                    "model": "rf_clf",
                    "cv_select": "train_all",
                    "params": {
                        "n_estimators": [3, 5, 10],
                        "max_depth": [3, 5, 10]
                    }
                },
                "threshold": 0.0,
                "add_column": false
            }
        }
    },
    "fit": {
        "myfunc": "titanic",
        "train_mode": "clf",
        "scoring": "accuracy",
        "cv": {
            "fold": "stratifiedk",
            "num": 3
        },
        "modelname": "tmp_titanic3",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "torch_clf",
                "missing_imputation": "mean",
                "x_scaler": "standard",
                "oversampling": "smote",
                "cv_select": "train_all",
                "fit_params": {
                },
                "params": {
                    "max_epochs": [3],
                    "batch_size": [50]
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
